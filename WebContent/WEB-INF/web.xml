<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>goodTrash</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  
  
  <!-- User FrontController Mapping -->
  <servlet>
     <servlet-name>User</servlet-name>
     <servlet-class>com.goodTrash.app.user.UserFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
     <servlet-name>User</servlet-name>
     <url-pattern>*.user</url-pattern>
  </servlet-mapping>
  
  <!-- Admin FrontController Mapping -->
  <servlet>
     <servlet-name>Admin</servlet-name>
     <servlet-class>com.goodTrash.app.admin.AdminFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
     <servlet-name>Admin</servlet-name>
     <url-pattern>*.admin</url-pattern>
  </servlet-mapping>
  
  <!-- Store FrontController Mapping -->
  <servlet>
     <servlet-name>Product</servlet-name>
     <servlet-class>com.goodTrash.app.product.ProductFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
     <servlet-name>Product</servlet-name>
     <url-pattern>*.product</url-pattern>
  </servlet-mapping>
  
  
  <!-- Driver FrontController Mapping -->
  <servlet>
     <servlet-name>Driver</servlet-name>
     <servlet-class>com.goodTrash.app.driver.DriverFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
     <servlet-name>Driver</servlet-name>
     <url-pattern>*.driver</url-pattern>
  </servlet-mapping>
  
  <!-- Notice FrontController Mapping -->
  <servlet>
     <servlet-name>Notice</servlet-name>
     <servlet-class>com.goodTrash.app.notice.NoticeFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
     <servlet-name>Notice</servlet-name>
     <url-pattern>*.notice</url-pattern>
  </servlet-mapping>
  
   
    <!-- MyPage FrontController Mapping -->
  <servlet>
     <servlet-name>MyPage</servlet-name>
     <servlet-class>com.goodTrash.app.myPage.MyPageFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
     <servlet-name>MyPage</servlet-name>
     <url-pattern>*.mp</url-pattern>
  </servlet-mapping>
  
    <!-- GarbageCollect FrontController Mapping -->
  <servlet>
     <servlet-name>GarbageCollect</servlet-name>
     <servlet-class>com.goodTrash.app.garbageCollect.GarbageCollectFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
     <servlet-name>GarbageCollect</servlet-name>
     <url-pattern>*.collect</url-pattern>
  </servlet-mapping>
  
  
  
  
  
  
</web-app>
